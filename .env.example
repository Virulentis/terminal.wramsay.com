# Resume Configuration
# Copy this file to .env.local and customize the values for your setup

# GitHub repository containing your resume YAML file
# Format: "username/repository-name"
VITE_RESUME_REPO=ahzs645/resume

# Git branch to fetch from (usually 'main' or 'master')
VITE_RESUME_BRANCH=main

# Path to the YAML file within the repository
VITE_RESUME_FILE_PATH=Ahmad_Jalil_CV.yaml

# GitHub Personal Access Token (required for private repositories)
# Create at: https://github.com/settings/tokens
# Needs 'repo' scope for private repositories, or 'public_repo' for public ones
# VITE_GITHUB_TOKEN=ghp_your_token_here

# Whether to fall back to local file if GitHub fetch fails
# Set to 'false' to disable local fallback
VITE_USE_LOCAL_FALLBACK=true
